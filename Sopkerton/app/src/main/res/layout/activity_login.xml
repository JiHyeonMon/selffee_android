<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="164dp"
        android:layout_height="69dp"
        android:layout_marginTop="188dp"
        android:layout_marginBottom="503dp"
        android:src="@drawable/selffee_logo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="44dp"
        android:layout_height="24dp"
        android:layout_marginTop="274dp"
        android:layout_marginBottom="462dp"
        android:layout_marginRight="158dp"
        android:layout_marginLeft="158dp"
        android:text="아이디"
        android:textColor="#515151"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editText"
        android:layout_width="270dp"
        android:layout_height="44dp"
        android:layout_marginTop="305dp"
        android:layout_marginBottom="411dp"
        android:layout_marginLeft="51dp"
        android:layout_marginRight="43dp"
        android:hint="아이디를 입력하세요"
        android:textSize="12sp"
        android:paddingVertical="13dp"
        android:paddingLeft="78dp"
        android:paddingRight="86dp"
        android:background="#f5f5f5"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:gravity="center_horizontal" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="58dp"
        android:layout_height="24dp"
        android:layout_marginHorizontal="20dp"
        android:layout_marginTop="383dp"
        android:layout_marginBottom="353dp"
        android:layout_marginLeft="151dp"
        android:layout_marginRight="151dp"
        android:text="비밀번호"
        android:textColor="#515151"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editText2"
        android:layout_width="266dp"
        android:layout_height="44dp"
        android:layout_marginHorizontal="50dp"
        android:layout_marginLeft="51dp"
        android:layout_marginTop="414dp"
        android:layout_marginRight="43dp"
        android:layout_marginBottom="302dp"
        android:background="#f5f5f5"
        android:gravity="center_horizontal"
        android:hint="비밀번호를 입력하세요"
        android:paddingVertical="13dp"
        android:paddingLeft="72dp"
        android:paddingRight="80dp"
        android:textAlignment="center"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="110dp"
        android:layout_height="18dp"
        android:layout_marginLeft="127dp"
        android:layout_marginTop="474dp"
        android:layout_marginRight="127dp"
        android:layout_marginBottom="268dp"
        android:text="@string/tv1"
        android:textColorHighlight="#9d9d9d"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:text="@string/tv2"
        android:layout_width="44dp"
        android:layout_height="18dp"
        android:textColor="#9d9d9d"
        android:textSize="12sp"
        android:layout_marginTop="634dp"
        android:layout_marginBottom="108dp"
        android:layout_marginLeft="158dp"
        android:layout_marginRight="158dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>